pragma solidity 0.8.22;
contract test{
    function f(uint a)public pure returns(string memory){
        if (a> 0 && a<10){
            return "the number is bigger than zero";
        }
        else if (a>10 && a<20){
            return "the number is bigger than ten";
        }
        else{
            return "the number is bigger than 20";
        }
    }
}
